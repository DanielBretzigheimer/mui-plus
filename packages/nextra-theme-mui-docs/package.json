{
  "name": "nextra-theme-mui-docs",
  "version": "0.0.4",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "5.0.0-alpha.35",
    "@material-ui/icons": "5.0.0-alpha.35",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "github-slugger": "^1.3.0",
    "next-themes": "^0.0.14",
    "prism-react-renderer": "^1.2.0",
    "react-innertext": "^1.1.5"
  },
  "devDependencies": {
    "@types/github-slugger": "^1.3.0",
    "@types/mdx-js__react": "^1.5.3",
    "microbundle": "^0.13.0"
  },
  "source": "./src/index.tsx",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "exports": {
    ".": "./dist/index.modern.js",
    "./CodeExample": "./dist/CodeExample.modern.js",
    "./Code": "./dist/Code.modern.js",
    "./Link": "./dist/Link.modern.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "microbundle --jsxFragment React.Fragment --jsx 'React.createElement' --format es,cjs,modern ./*.ts",
    "dev": "microbundle watch --jsxFragment React.Fragment --jsx 'React.createElement' --no-compress --raw --format es,cjs,modern ./*.ts",
    "prepare": "yarn build",
    "pub": "yarn publish"
  },
  "peerDependencies": {
    "next": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
