{
  "name": "site-search",
  "version": "1.0.0",
  "dependencies": {
    "ajv": "^8.5.0",
    "execa": "^5.0.1",
    "jsdom": "^16.6.0",
    "lunr": "^2.3.9",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.10",
    "@types/lunr": "^2.3.3",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "microbundle": "^0.13.1"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "exports": {
    ".": "./dist/index.modern.js",
    "./handler": "./dist/handler.modern.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "microbundle --jsxFragment React.Fragment --jsx 'React.createElement' --format es,cjs,modern ./*.ts",
    "dev": "microbundle watch --jsxFragment React.Fragment --jsx 'React.createElement' --no-compress --raw --format es,cjs,modern ./*.ts"
  },
  "bin": {
    "site-search": "./bin/cli.js"
  },
  "peerDependencies": {}
}
