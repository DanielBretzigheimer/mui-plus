  "license": "MIT",
  "devDependencies": {
    "@types/resize-observer-browser": "^0.1.5",
    "microbundle": "^0.13.0",
    "react": "^17.0.1",
    "typescript": "^4.1.5"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
